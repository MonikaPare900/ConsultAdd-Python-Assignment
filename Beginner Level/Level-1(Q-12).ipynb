   "source": [
    "# Reverse the digit\n",
    "num = 1234\n",
    "reversed_num = 0\n",
    "\n",
    "while num > 0:\n",
    "    print(num)\n",
    "    \n",
    "    digit = num % 10\n",
    "    print(digit)\n",
    "    reversed_num = reversed_num * 10 + digit\n",
    "    print(reversed_num)\n",
    "    num //= 10\n",
    "\n",
    "print(\"Reversed Number: \" + str(reversed_num))"
   ]
