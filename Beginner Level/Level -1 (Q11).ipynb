    "#Sum of the digits\n",
    "num = int(input())\n",
    "sum=0\n",
    "def sumofdigit(num,sum):\n",
    "    while num>0 or sum>9:\n",
    "        if num==0:\n",
    "           \n",
    "            num=sum\n",
    "            sum=0\n",
    "        sum+=num%10\n",
    "        num//=10\n",
    "        print(num)\n",
    "        print(sum)\n",
    "        \n",
    "sumofdigit(num,sum)        "
