    "#Pile Game\n",
    "n = int(input())\n",
    "def stone_piles(n):\n",
    "    final = []\n",
    "    if n % 2 == 0:\n",
    "        start =2\n",
    "    else:\n",
    "        start = 1\n",
    "    \n",
    "    while start<=n:\n",
    "        final.append(start)\n",
    "        start+=2\n",
    "        #print(final)\n",
    "    return final\n",
    "            \n",
    "stone_piles(n)  \n"
   ]
