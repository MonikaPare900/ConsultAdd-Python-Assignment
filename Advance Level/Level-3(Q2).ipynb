   "source": [
    "2. Write a program to count the lines in a file “demo.txt”"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "4\n"
     ]
    }
   ],
   "source": [
    "with open('Demo.txt', 'r') as file:\n",
    "    lines = file.readlines()  # Read all lines\n",
    "Result= 0\n",
    "for line in lines:\n",
    "    if line.strip():\n",
    "        Result+=1\n",
    "\n",
    "print(Result)   \n",
